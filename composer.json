{
    "name": "gewis/gewisdb",
    "description": "GEWIS Database management",
    "license": "GPL-3.0",
    "keywords": [
        "members",
        "decisions",
        "gewis"
    ],
    "homepage": "http://gewis.nl/",
    "require": {
        "php": "^8.1.0",
        "ext-intl": "*",
        "ext-memcached": "*",
        "ext-pdo_pgsql": "*",
        "ext-pgsql": "*",
        "ext-zend-opcache": "*",
        "ext-zip": "^1.12.0",
        "laminas/laminas-authentication": "^2.8.0",
        "laminas/laminas-cache": "^3.4.0",
        "laminas/laminas-cache-storage-adapter-memcached": "^2.1.0",
        "laminas/laminas-config": "^3.4.0",
        "laminas/laminas-cli": "^1.1.0",
        "laminas/laminas-crypt": "^3.6.0",
        "laminas/laminas-eventmanager": "^3.4.0",
        "laminas/laminas-filter": "*",
        "laminas/laminas-form": "^3.0.2",
        "laminas/laminas-http": "^2.15.0",
        "laminas/laminas-hydrator": "*",
        "laminas/laminas-i18n": "^2.13.0",
        "laminas/laminas-i18n-resources": "^2.8.0",
        "laminas/laminas-inputfilter": "^2.12.1",
        "laminas/laminas-json": "*",
        "laminas/laminas-mail": "^2.16.0",
        "laminas/laminas-mime": "^2.9.1",
        "laminas/laminas-modulemanager": "^2.11.0",
        "laminas/laminas-mvc": "^3.3.3",
        "laminas/laminas-mvc-plugin-identity": "^1.4.0",
        "laminas/laminas-mvc-i18n": "^1.3.1",
        "laminas/laminas-progressbar": "^2.10.0",
        "laminas/laminas-router": "^3.5.0",
        "laminas/laminas-session": "^2.12.1",
        "laminas/laminas-serializer": "^2.12.0",
        "laminas/laminas-servicemanager": "^3.7.0",
        "laminas/laminas-stdlib": "^3.10.1",
        "laminas/laminas-validator": "*",
        "laminas/laminas-view": "^2.16.0",
        "doctrine/dbal": "^3.3.7",
        "doctrine/orm": "^2.12.2",
        "doctrine/common": "^3.3.0",
        "doctrine/doctrine-module": "^5.2.0",
        "doctrine/doctrine-orm-module": "^5.2.0",
        "doctrine/doctrine-laminas-hydrator": "^3.1.0",
        "monolog/monolog": "^1.27",
        "cweagans/composer-patches": "^1.7.2"
    },
    "require-dev": {
        "laminas/laminas-component-installer": "^2.6.0",
        "laminas/laminas-developer-tools": "^2.3.0",
        "laminas/laminas-loader": "^2.8.0",
        "laminas/laminas-test": "^4.0.0",
        "san/san-session-toolbar": "^4.0.0",
        "phpunit/phpunit": "^9.5.20",
        "squizlabs/php_codesniffer": "^3.6.2",
        "friendsofphp/php-cs-fixer": "^3.8.0",
        "phpstan/phpstan": "^1.7.12",
        "phpstan/extension-installer": "^1.1.0",
        "bnf/phpstan-psr-container": "^1.0.1",
        "slam/phpstan-laminas-framework": "^1.2.0",
        "phpstan/phpstan-doctrine": "^1.3.9",
        "phpstan/phpstan-phpunit": "^1.1.1",
        "vimeo/psalm": "^4.23.0",
        "lctrs/psalm-psr-container-plugin": "^1.7.0",
        "weirdan/doctrine-psalm-plugin": "^2.3.0",
        "psalm/plugin-phpunit": "^0.16.1",
        "maglnet/composer-require-checker": "^4.0.0",
        "icanhazstring/composer-unused": "^0.8.2"
    },
    "config": {
        "preferred-install": {
            "doctrine/orm": "source",
            "*": "dist"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "laminas/laminas-dependency-plugin": true,
            "cweagans/composer-patches": true,
            "laminas/laminas-component-installer": true,
            "phpstan/extension-installer": true,
            "icanhazstring/composer-unused": true
        },
        "discard-changes" : true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "patches": {
            "doctrine/orm": {
                "Fix issues with SubDecisions.": "https://raw.githubusercontent.com/GEWIS/orm/2.12.x/1-to-1-multiple-join-columns.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src",
            "Checker\\": "module/Checker/src",
            "Database\\": "module/Database/src",
            "Report\\": "module/Report/src",
            "User\\": "module/User/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test",
            "CheckerTest\\": "module/Checker/test"
        }
    }
}
